{
    "version": 2,
    "name": "novadis-cri",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/service-worker.js",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/service-worker.js"
        },
        {
            "src": "/manifest.json",
            "headers": {
                "content-type": "application/json"
            },
            "dest": "/manifest.json"
        },
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}